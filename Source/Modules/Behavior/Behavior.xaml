                        <Image Grid.Column="2" Source="{Binding User5Star3}" Height="32"/>
                        <Image Grid.Column="3" Source="{Binding User5Star4}" Height="32"/>
                        <Image Grid.Column="4" Source="{Binding User5Star5}" Height="32"/>
                    </Grid>

                    <Grid Grid.Row="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <Image Grid.Column="0" Source="{Binding User5Strike1}"/>
                        <Image Grid.Column="1" Source="{Binding User5Strike2}"/>
                        <Image Grid.Column="2" Source="{Binding User5Strike3}"/>
                    </Grid>

                    <TextBlock Grid.Row="4" Text="{Binding User5CashAvailable}" FontSize="32" Foreground="{Binding User5CashAvailableColor}" HorizontalAlignment="Center"/>
                    <TextBlock Grid.Row="5" Text="{Binding User5CashReleased}" FontSize="14" FontStyle="Italic" Foreground="Green" HorizontalAlignment="Center"/>

                    <Grid Grid.Row="6" Margin="4">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Row="0" Text="{Binding CurrentDayText}" FontSize="16" HorizontalAlignment="Center"/>
                        <ProgressBar Grid.Row="1" Value="{Binding User5TasksCompletedDayProgressValue}" IsIndeterminate="False" Foreground="{Binding User5TasksCompletedDayProgressColor}"
                                     Background="Transparent" BorderBrush="DarkSlateGray" BorderThickness="1"/>
                        <TextBlock Grid.Row="1" Text="{Binding User5TasksCompletedDayProgressText}" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Grid>


                    <Grid Grid.Row="7" Margin="4">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Row="0" Text="{Binding CurrentWeekText}" HorizontalAlignment="Center" FontSize="16"/>
                        <ProgressBar Grid.Row="1" Value="{Binding User5TasksCompletedWeekProgressValue}" IsIndeterminate="False" Foreground="{Binding User5TasksCompletedWeekProgressColor}"
                                     Background="Transparent" BorderBrush="DarkSlateGray" BorderThickness="1"/>
                        <TextBlock Grid.Row="1" Text="{Binding User5TasksCompletedWeekProgressText}" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Grid>

                    <Grid Grid.Row="8" Margin="4">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Row="0" Text="{Binding CurrentMonthText}" FontSize="16" HorizontalAlignment="Center"/>
                        <ProgressBar Grid.Row="1" Value="{Binding User5TasksCompletedMonthProgressValue}" IsIndeterminate="False" Foreground="{Binding User5TasksCompletedMonthProgressColor}"
                                     Background="Transparent" BorderBrush="DarkSlateGray" BorderThickness="1"/>
                        <TextBlock Grid.Row="1" Text="{Binding User5TasksCompletedMonthProgressText}" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>

                    </Grid>

                    <Grid Grid.Row="9" Margin="4">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Row="0" Text="{Binding CurrentQuarterText}" FontSize="16" HorizontalAlignment="Center"/>
                        <ProgressBar Grid.Row="1" Value="{Binding User5TasksCompletedQuarterProgressValue}" IsIndeterminate="False" Foreground="{Binding User5TasksCompletedQuarterProgressColor}"
                                     Background="Transparent" BorderBrush="DarkSlateGray" BorderThickness="1"/>
                        <TextBlock Grid.Row="1" Text="{Binding User5TasksCompletedQuarterProgressText}" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Grid>
                </Grid>
            </Button>
        </Grid>
    </Border>
</Page>