                        <TextBlock Grid.Row="1" Text="{Binding User5ChoresCompletedMonthProgressText}" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>

                    </Grid>

                    <Grid Grid.Row="7" Margin="4">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Row="0" Text="{Binding CurrentQuarterText}" FontSize="16" HorizontalAlignment="Center"/>
                        <ProgressBar Grid.Row="1" Value="{Binding User5ChoresCompletedQuarterProgressValue}" IsIndeterminate="False" Foreground="CornflowerBlue"
                                     Background="Transparent" BorderBrush="DarkSlateGray" BorderThickness="1"/>
                        <TextBlock Grid.Row="1" Text="{Binding User5ChoresCompletedQuarterProgressText}" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Grid>
                </Grid>
            </Button>
        </Grid>
    </Border>
</Page>