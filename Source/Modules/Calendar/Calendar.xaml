                                        </Border>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </Button.Style>
                    <DockPanel Focusable="False">
                        <Grid DockPanel.Dock="Top" Focusable="False" Margin="2 2 2 0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Column="0" Text="{Binding Button1Date}" FontWeight="Bold"/>
                            <TextBlock Grid.Column="1" Text="{Binding Button1HolidayText}" Foreground="Red" FontWeight="Bold" HorizontalAlignment="Right"/>
                        </Grid>

