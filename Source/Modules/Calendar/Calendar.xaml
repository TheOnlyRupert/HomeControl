<Page
    x:Class="HomeControl.Source.Modules.Calendar.Calendar"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    mc:Ignorable="d">
    <Page.Resources>
        <Style
            x:Key="SliderButtonStyle"
            TargetType="{x:Type RepeatButton}">
            <Setter
                Property="SnapsToDevicePixels"
                Value="true"/>
            <Setter
                Property="OverridesDefaultStyle"
                Value="true"/>
            <Setter
                Property="IsTabStop"
                Value="false"/>
            <Setter
                Property="Focusable"
                Value="false"/>
            <Setter
                Property="Template">
                <Setter.Value>
                    <ControlTemplate
                        TargetType="{x:Type RepeatButton}">
                        <Border
                            Background="Transparent"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style
            x:Key="SliderThumbStyle"
            TargetType="{x:Type Thumb}">
            <Setter
                Property="SnapsToDevicePixels"
                Value="true"/>
            <Setter
                Property="OverridesDefaultStyle"
                Value="true"/>
            <Setter
                Property="Template">
                <Setter.Value>
                    <ControlTemplate
                        TargetType="{x:Type Thumb}">
                        <Border
                            BorderThickness="0"
                            CornerRadius="8"
                            Background="Gray"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <ControlTemplate
            x:Key="VerticalScrollBar"
            TargetType="{x:Type ScrollBar}">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition
                        Width="Auto"/>
                    <ColumnDefinition
                        Width="Auto"
                        MinWidth="{TemplateBinding MinWidth}"/>
                    <ColumnDefinition
                        Width="Auto"/>
                </Grid.ColumnDefinitions>

                <TickBar
                    Grid.Column="0"
                    x:Name="TopTick"
                    SnapsToDevicePixels="True"
                    Placement="Left"
                    Width="4"
                    Visibility="Collapsed"/>

                <Track
                    Grid.Column="1"
                    x:Name="PART_Track">
                    <Track.DecreaseRepeatButton>
                        <RepeatButton
                            Style="{StaticResource SliderButtonStyle}"
                            Command="Slider.DecreaseLarge"/>
                    </Track.DecreaseRepeatButton>
                    <Track.Thumb>
                        <Thumb
                            Style="{StaticResource SliderThumbStyle}"/>
                    </Track.Thumb>
                </Track>

                <TickBar
                    x:Name="BottomTick"
                    SnapsToDevicePixels="True"
                    Grid.Column="2"
                    Placement="Right"
                    Width="4"
                    Visibility="Collapsed"/>
            </Grid>
        </ControlTemplate>

        <Style
            TargetType="{x:Type ScrollBar}">
            <Setter
                Property="SnapsToDevicePixels"
                Value="true"/>
            <Setter
                Property="OverridesDefaultStyle"
                Value="true"/>
            <Style.Triggers>
                <Trigger
                    Property="Orientation"
                    Value="Vertical">
                    <Setter
                        Property="MinWidth"
                        Value="8"/>
                    <Setter
                        Property="Template"
                        Value="{StaticResource VerticalScrollBar}"/>
                </Trigger>
            </Style.Triggers>
        </Style>
    </Page.Resources>

    <Grid
        Margin="4 4 0 0">
        <Grid.RowDefinitions>
            <RowDefinition
                Height="Auto"/>
            <RowDefinition
                Height="Auto"/>
            <RowDefinition
                Height="*"/>
        </Grid.RowDefinitions>

        <Grid
            Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="1.2*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
            </Grid.ColumnDefinitions>

            <Button
                Grid.Column="0"
                Content="⚙"
                Command="{Binding ButtonCommand}"
                FontSize="18"
                CommandParameter="recurring"
                HorizontalAlignment="Left">
                <Button.Style>
                    <Style
                        TargetType="{x:Type Button}">
                        <Setter
                            Property="Background"
                            Value="Transparent"/>
                        <Setter
                            Property="Foreground"
                            Value="White"/>
                        <Setter
                            Property="Template">
                            <Setter.Value>
                                <ControlTemplate
                                    TargetType="{x:Type Button}">
                                    <Border
                                        Background="{TemplateBinding Background}"
                                        BorderBrush="DarkSlateGray"
                                        BorderThickness="1"
                                        Padding="12 4">
                                        <ContentPresenter
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"/>
                                    </Border>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Button.Style>
            </Button>

            <TextBlock
                Grid.Column="0"
                Grid.ColumnSpan="2"
                HorizontalAlignment="Center"
                Foreground="Red"
                FontWeight="Bold"
                FontSize="18"/>

            <Button
                Grid.Column="1"
                Content="&lt;"
                Command="{Binding ButtonCommand}"
                FontSize="18"
                HorizontalContentAlignment="Center"
                HorizontalAlignment="Right"
                CommandParameter="subMonth">
                <Button.Style>
                    <Style
                        TargetType="{x:Type Button}">
                        <Setter
                            Property="Background"
                            Value="Transparent"/>
                        <Setter
                            Property="Foreground"
                            Value="White"/>
                        <Setter
                            Property="Template">
                            <Setter.Value>
                                <ControlTemplate
                                    TargetType="{x:Type Button}">
                                    <Border
                                        Background="{TemplateBinding Background}"
                                        BorderBrush="DarkSlateGray"
                                        BorderThickness="1"
                                        Padding="16 4">
                                        <ContentPresenter
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"/>
                                    </Border>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Button.Style>
            </Button>

            <TextBlock
                Grid.Column="2"
                Text="{Binding CurrentMonthAndYear}"
                Foreground="White"
                FontSize="24"
                HorizontalAlignment="Center"/>

            <StackPanel
                Grid.Column="3"
                Orientation="Horizontal">
                <Button
                    Content="&gt;"
                    Command="{Binding ButtonCommand}"
                    FontSize="18"
                    CommandParameter="addMonth">
                    <Button.Style>
                        <Style
                            TargetType="{x:Type Button}">
                            <Setter
                                Property="Background"
                                Value="Transparent"/>
                            <Setter
                                Property="Foreground"
                                Value="White"/>
                            <Setter
                                Property="Template">
                                <Setter.Value>
                                    <ControlTemplate
                                        TargetType="{x:Type Button}">
                                        <Border
                                            Background="{TemplateBinding Background}"
                                            BorderBrush="DarkSlateGray"
                                            BorderThickness="1"
                                            Padding="16 4">
                                            <ContentPresenter
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"/>
                                        </Border>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </Button.Style>
                </Button>

                <Button
                    Content="📅"
                    Command="{Binding ButtonCommand}"
                    FontSize="18"
                    CommandParameter="today"
                    Margin="16 0 0 0">
                    <Button.Style>
                        <Style
                            TargetType="{x:Type Button}">
                            <Setter
                                Property="Background"
                                Value="Transparent"/>
                            <Setter
                                Property="Foreground"
                                Value="White"/>
                            <Setter
                                Property="Template">
                                <Setter.Value>
                                    <ControlTemplate
                                        TargetType="{x:Type Button}">
                                        <Border
                                            Background="{TemplateBinding Background}"
                                            BorderBrush="DarkSlateGray"
                                            BorderThickness="1"
                                            Padding="12 4">
                                            <ContentPresenter
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"/>
                                        </Border>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </Button.Style>
                </Button>
            </StackPanel>

            <Button
                Grid.Column="4"
                Content="∀"
                Command="{Binding ButtonCommand}"
                FontSize="18"
                CommandParameter="filter"
                HorizontalAlignment="Right">
                <Button.Style>
                    <Style
                        TargetType="{x:Type Button}">
                        <Setter
                            Property="Background"
                            Value="{Binding FilterBackgroundColor}"/>
                        <Setter
                            Property="Foreground"
                            Value="White"/>
                        <Setter
                            Property="Template">
                            <Setter.Value>
                                <ControlTemplate
                                    TargetType="{x:Type Button}">
                                    <Border
                                        Background="{TemplateBinding Background}"
                                        BorderBrush="DarkSlateGray"
                                        BorderThickness="1"
                                        Padding="16 4">
                                        <ContentPresenter
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"/>
                                    </Border>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Button.Style>
            </Button>
        </Grid>

        <Grid
            Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
            </Grid.ColumnDefinitions>

            <TextBlock
                Grid.Column="0"
                Text="Sun"
                FontSize="16"
                Foreground="White"
                HorizontalAlignment="Center"/>
            <TextBlock
                Grid.Column="1"
                Text="Mon"
                FontSize="16"
                Foreground="White"
                HorizontalAlignment="Center"/>
            <TextBlock
                Grid.Column="2"
                Text="Tue"
                FontSize="16"
                Foreground="White"
                HorizontalAlignment="Center"/>
            <TextBlock
                Grid.Column="3"
                Text="Wed"
                FontSize="16"
                Foreground="White"
                HorizontalAlignment="Center"/>
            <TextBlock
                Grid.Column="4"
                Text="Thur"
                FontSize="16"
                Foreground="White"
                HorizontalAlignment="Center"/>
            <TextBlock
                Grid.Column="5"
                Text="Fri"
                FontSize="16"
                Foreground="White"
                HorizontalAlignment="Center"/>
            <TextBlock
                Grid.Column="6"
                Text="Sat"
                FontSize="16"
                Foreground="White"
                HorizontalAlignment="Center"/>
        </Grid>

        <Grid
            Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
                <ColumnDefinition
                    Width="*"/>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition
                    Height="*"/>
                <RowDefinition
                    Height="*"/>
                <RowDefinition
                    Height="*"/>
                <RowDefinition
                    Height="*"/>
                <RowDefinition
                    Height="*"/>
                <RowDefinition
                    Height="*"/>
            </Grid.RowDefinitions>

            <Grid
                Grid.Row="0"
                Grid.Column="0">
                <Border
                    BorderBrush="{Binding Button1BorderColor}"
                    BorderThickness="{Binding Button1BorderThickness}">
                    <DockPanel
                        Focusable="False">
                        <Grid
                            DockPanel.Dock="Top"
                            Focusable="False"
                            Margin="2 2 2 0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="Auto"/>
                                <ColumnDefinition
                                    Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock
                                Grid.Column="0"
                                Text="{Binding Button1Date}"
                                FontWeight="Bold"
                                Foreground="White"/>
                            <TextBlock
                                Grid.Column="1"
                                Text="{Binding Button1HolidayText}"
                                Foreground="Red"
                                FontWeight="Bold"
                                HorizontalAlignment="Right"/>
                        </Grid>

                        <ListView
                            ItemsSource="{Binding Button1EventList}"
                            Foreground="White"
                            Background="Transparent"
                            BorderThickness="0"
                            Focusable="False"
                            FontSize="8"
                            Margin="-4 0 0 0">
                            <ListView.Resources>
                                <Style
                                    TargetType="GridViewColumnHeader">
                                    <Setter
                                        Property="Visibility"
                                        Value="Collapsed"/>
                                </Style>

                                <Style
                                    TargetType="ListViewItem">
                                    <Style.Triggers>
                                        <DataTrigger
                                            Binding="{Binding Priority}"
                                            Value="0">
                                            <Setter
                                                Property="Foreground"
                                                Value="White"/>
                                            <Setter
                                                Property="Focusable"
                                                Value="False"/>
                                        </DataTrigger>

                                        <DataTrigger
                                            Binding="{Binding Priority}"
                                            Value="1">
                                            <Setter
                                                Property="Foreground"
                                                Value="CornflowerBlue"/>
                                            <Setter
                                                Property="Focusable"
                                                Value="False"/>
                                        </DataTrigger>

                                        <DataTrigger
                                            Binding="{Binding Priority}"
                                            Value="2">
                                            <Setter
                                                Property="Foreground"
                                                Value="Green"/>
                                            <Setter
                                                Property="Focusable"
                                                Value="False"/>
                                        </DataTrigger>

                                        <MultiTrigger>
                                            <MultiTrigger.Conditions>
                                                <Condition
                                                    Property="IsSelected"
                                                    Value="True"/>
                                            </MultiTrigger.Conditions>
                                            <MultiTrigger.Setters>
                                                <Setter
                                                    Property="Background"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="BorderBrush"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="Foreground"
                                                    Value="White"/>
                                                <Setter
                                                    Property="Focusable"
                                                    Value="False"/>
                                            </MultiTrigger.Setters>
                                        </MultiTrigger>
                                        <MultiTrigger>
                                            <MultiTrigger.Conditions>
                                                <Condition
                                                    Property="IsMouseOver"
                                                    Value="True"/>
                                            </MultiTrigger.Conditions>
                                            <MultiTrigger.Setters>
                                                <Setter
                                                    Property="Background"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="BorderBrush"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="Foreground"
                                                    Value="White"/>
                                                <Setter
                                                    Property="Focusable"
                                                    Value="False"/>
                                            </MultiTrigger.Setters>
                                        </MultiTrigger>
                                    </Style.Triggers>
                                </Style>
                            </ListView.Resources>

                            <ListView.View>
                                <GridView
                                    AllowsColumnReorder="false">
                                    <GridViewColumn
                                        Width="24">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Image
                                                    Source="{Binding Image}"
                                                    Height="24"
                                                    Margin="-4 -4 -4 0"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>

                                    <GridViewColumn
                                        Width="128">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock
                                                    Text="{Binding Description}"
                                                    FontSize="8"
                                                    Margin="-6 0 0 0"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                </GridView>
                            </ListView.View>
                        </ListView>
                    </DockPanel>
                </Border>
            </Grid>
            <Button
                Grid.Row="0"
                Grid.Column="0"
                Command="{Binding ButtonCommand}"
                CommandParameter="button1">
                <Button.Style>
                    <Style
                        TargetType="{x:Type Button}">
                        <Setter
                            Property="Background"
                            Value="Transparent"/>
                        <Setter
                            Property="Template">
                            <Setter.Value>
                                <ControlTemplate
                                    TargetType="{x:Type Button}">
                                    <Border
                                        Background="Transparent"
                                        BorderBrush="Transparent"
                                        BorderThickness="0">
                                        <ContentPresenter
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"/>
                                    </Border>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Button.Style>
            </Button>

            <Grid
                Grid.Row="0"
                Grid.Column="1">
                <Border
                    BorderBrush="{Binding Button2BorderColor}"
                    BorderThickness="{Binding Button2BorderThickness}">
                    <DockPanel
                        Focusable="False">
                        <Grid
                            DockPanel.Dock="Top"
                            Focusable="False"
                            Margin="2 2 2 0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="Auto"/>
                                <ColumnDefinition
                                    Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock
                                Grid.Column="0"
                                Text="{Binding Button2Date}"
                                FontWeight="Bold"
                                Foreground="White"/>
                            <TextBlock
                                Grid.Column="1"
                                Text="{Binding Button2HolidayText}"
                                Foreground="Red"
                                FontWeight="Bold"
                                HorizontalAlignment="Right"/>
                        </Grid>

                        <ListView
                            ItemsSource="{Binding Button2EventList}"
                            Foreground="White"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            BorderThickness="0"
                            Focusable="False"
                            FontSize="8"
                            DockPanel.Dock="Bottom"
                            Margin="-4 0 0 0">
                            <ListView.Resources>
                                <Style
                                    TargetType="GridViewColumnHeader">
                                    <Setter
                                        Property="Visibility"
                                        Value="Collapsed"/>
                                </Style>

                                <Style
                                    TargetType="ListViewItem">
                                    <Style.Triggers>
                                        <DataTrigger
                                            Binding="{Binding Priority}"
                                            Value="0">
                                            <Setter
                                                Property="Foreground"
                                                Value="White"/>
                                            <Setter
                                                Property="Focusable"
                                                Value="False"/>
                                        </DataTrigger>

                                        <DataTrigger
                                            Binding="{Binding Priority}"
                                            Value="1">
                                            <Setter
                                                Property="Foreground"
                                                Value="CornflowerBlue"/>
                                            <Setter
                                                Property="Focusable"
                                                Value="False"/>
                                        </DataTrigger>

                                        <DataTrigger
                                            Binding="{Binding Priority}"
                                            Value="2">
                                            <Setter
                                                Property="Foreground"
                                                Value="Green"/>
                                            <Setter
                                                Property="Focusable"
                                                Value="False"/>
                                        </DataTrigger>

                                        <MultiTrigger>
                                            <MultiTrigger.Conditions>
                                                <Condition
                                                    Property="IsSelected"
                                                    Value="True"/>
                                            </MultiTrigger.Conditions>
                                            <MultiTrigger.Setters>
                                                <Setter
                                                    Property="Background"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="BorderBrush"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="Focusable"
                                                    Value="False"/>
                                            </MultiTrigger.Setters>
                                        </MultiTrigger>
                                        <MultiTrigger>
                                            <MultiTrigger.Conditions>
                                                <Condition
                                                    Property="IsMouseOver"
                                                    Value="True"/>
                                            </MultiTrigger.Conditions>
                                            <MultiTrigger.Setters>
                                                <Setter
                                                    Property="Background"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="BorderBrush"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="Focusable"
                                                    Value="False"/>
                                            </MultiTrigger.Setters>
                                        </MultiTrigger>
                                    </Style.Triggers>
                                </Style>
                            </ListView.Resources>
                            <ListView.View>
                                <GridView
                                    AllowsColumnReorder="false">
                                    <GridViewColumn
                                        Width="24">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Image
                                                    Source="{Binding Image}"
                                                    Height="24"
                                                    Margin="-4 -4 -4 0"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>

                                    <GridViewColumn
                                        Width="128">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock
                                                    Text="{Binding Description}"
                                                    FontSize="8"
                                                    Margin="-6 0 0 0"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                </GridView>
                            </ListView.View>
                        </ListView>
                    </DockPanel>
                </Border>
            </Grid>
            <Button
                Grid.Row="0"
                Grid.Column="1"
                Command="{Binding ButtonCommand}"
                CommandParameter="button2">
                <Button.Style>
                    <Style
                        TargetType="{x:Type Button}">
                        <Setter
                            Property="Background"
                            Value="Transparent"/>
                        <Setter
                            Property="Template">
                            <Setter.Value>
                                <ControlTemplate
                                    TargetType="{x:Type Button}">
                                    <Border
                                        Background="Transparent"
                                        BorderBrush="Transparent"
                                        BorderThickness="0">
                                        <ContentPresenter
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"/>
                                    </Border>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Button.Style>
            </Button>

            <Grid
                Grid.Row="0"
                Grid.Column="2">
                <Border
                    BorderBrush="{Binding Button3BorderColor}"
                    BorderThickness="{Binding Button3BorderThickness}">
                    <DockPanel
                        Focusable="False">
                        <Grid
                            DockPanel.Dock="Top"
                            Focusable="False"
                            Margin="2 2 2 0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="Auto"/>
                                <ColumnDefinition
                                    Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock
                                Grid.Column="0"
                                Text="{Binding Button3Date}"
                                FontWeight="Bold"
                                Foreground="White"/>
                            <TextBlock
                                Grid.Column="1"
                                Text="{Binding Button3HolidayText}"
                                Foreground="Red"
                                FontWeight="Bold"
                                HorizontalAlignment="Right"/>
                        </Grid>

                        <ListView
                            ItemsSource="{Binding Button3EventList}"
                            Foreground="White"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            BorderThickness="0"
                            Focusable="False"
                            FontSize="8"
                            DockPanel.Dock="Bottom"
                            Margin="-4 0 0 0">
                            <ListView.Resources>
                                <Style
                                    TargetType="GridViewColumnHeader">
                                    <Setter
                                        Property="Visibility"
                                        Value="Collapsed"/>
                                </Style>

                                <Style
                                    TargetType="ListViewItem">
                                    <Style.Triggers>
                                        <DataTrigger
                                            Binding="{Binding Priority}"
                                            Value="0">
                                            <Setter
                                                Property="Foreground"
                                                Value="White"/>
                                            <Setter
                                                Property="Focusable"
                                                Value="False"/>
                                        </DataTrigger>

                                        <DataTrigger
                                            Binding="{Binding Priority}"
                                            Value="1">
                                            <Setter
                                                Property="Foreground"
                                                Value="CornflowerBlue"/>
                                            <Setter
                                                Property="Focusable"
                                                Value="False"/>
                                        </DataTrigger>

                                        <DataTrigger
                                            Binding="{Binding Priority}"
                                            Value="2">
                                            <Setter
                                                Property="Foreground"
                                                Value="Green"/>
                                            <Setter
                                                Property="Focusable"
                                                Value="False"/>
                                        </DataTrigger>

                                        <MultiTrigger>
                                            <MultiTrigger.Conditions>
                                                <Condition
                                                    Property="IsSelected"
                                                    Value="True"/>
                                            </MultiTrigger.Conditions>
                                            <MultiTrigger.Setters>
                                                <Setter
                                                    Property="Background"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="BorderBrush"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="Focusable"
                                                    Value="False"/>
                                            </MultiTrigger.Setters>
                                        </MultiTrigger>
                                        <MultiTrigger>
                                            <MultiTrigger.Conditions>
                                                <Condition
                                                    Property="IsMouseOver"
                                                    Value="True"/>
                                            </MultiTrigger.Conditions>
                                            <MultiTrigger.Setters>
                                                <Setter
                                                    Property="Background"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="BorderBrush"
                                                    Value="Transparent"/>
                                                <Setter
                                                    Property="Focusable"
                                                    Value="False"/>
                                            </MultiTrigger.Setters>
                                        </MultiTrigger>
                                    </Style.Triggers>
                                </Style>
                            </ListView.Resources>
                            <ListView.View>
                                <GridView
                                    AllowsColumnReorder="false">
                                    <GridViewColumn
                                        Width="24">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Image
                                                    Source="{Binding Image}"
                                                    Height="24"
                                                    Margin="-4 -4 -4 0"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>

                                    <GridViewColumn
                                        Width="128">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock
                                                    Text="{Binding Description}"
                                                    FontSize="8"
                                                    Margin="-6 0 0 0"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                </GridView>
                            </ListView.View>
                        </ListView>
                    </DockPanel>
                </Border>
            </Grid>
            <Button
                Grid.Row="0"
                Grid.Column="2"
                Command="{Binding ButtonCommand}"
                CommandParameter="button3">
                <Button.Style>
                    <Style
                        TargetType="{x:Type Button}">
                        <Setter
                            Property="Background"
                            Value="Transparent"/>
                        <Setter
                            Property="Template">
                            <Setter.Value>
                                <ControlTemplate
                                    TargetType="{x:Type Button}">
                                    <Border
                                        Background="Transparent"
                                        BorderBrush="Transparent"
                                        BorderThickness="0">
                                        <ContentPresenter
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"/>
                                    </Border>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Button.Style>
            </Button>

